<?xml version="1.0" encoding="windows-1251"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="Base"
	OutputDirectory="$(ConfigurationName)\$(PlatformName)"
	IntermediateDirectory="$(ConfigurationName)\$(PlatformName)"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(OWN_LIB_PATH)\Include;$(OWN_LIB_PATH)\FarLib"
		PreprocessorDefinitions="WIN32;FAR_USE_NAMESPACE;PCRE_STATIC;HAVE_CONFIG_H;NONDLL;_CRT_SECURE_NO_WARNINGS;_CRT_NON_CONFORMING_SWPRINTFS"
		StringPooling="true"
		UsePrecompiledHeader="2"
		WarningLevel="3"
		Detect64BitPortabilityProblems="false"
	/>
	<Tool
		Name="VCLibrarianTool"
		OutputFile="$(OWN_LIB_PATH)\Lib\$(ProjectName)$(LibSuffix)$(LibPlatform)$(LibAppend).lib"
	/>
	<Tool
		Name="VCLinkerTool"
		IgnoreImportLibrary="true"
		LinkLibraryDependencies="false"
		AdditionalLibraryDirectories="$(OWN_LIB_PATH)\Lib"
		GenerateDebugInformation="true"
		SubSystem="2"
		OptimizeForWindows98="1"
	/>
</VisualStudioPropertySheet>
